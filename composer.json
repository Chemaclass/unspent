{
    "name": "chemaclass/unspent",
    "description": "A PHP library for UTXO-like bookkeeping using unspent entries.",
    "type": "library",
    "homepage": "https://phel-lang.org/",
    "require": {
        "php": ">=8.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.5"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Chemaclass\\Unspent\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Jose M. Valera Reales",
            "email": "hi@chemaclass.es",
            "homepage": "https://chemaclass.com"
        }
    ],
    "minimum-stability": "stable",
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "ergebnis/composer-normalize": true
        },
        "platform": {
            "php": "8.4"
        },
        "sort-packages": true
    }
}
